# Pacman
alias pacman='sudo pacman'
alias pkgupdate='sudo pacman -Syu'
alias pkgclear='sudo pacman -R $(pacman -Qdtq)'

# Tippfehler etc
alias mkae='make'
alias vim='nvim'
alias pping='ping -c 5 8.8.8.8'

# configs
alias i3conf='nvim ~/.config/i3/config'
alias polyconf='nvim ~/.config/polybar/config'

# Dirs
alias p='cd ~/Programms'
alias c='cd ~/Coding'
alias h='cd ~'
alias d='cd ~/Coding/dotdotdot'
alias s='cd ~/.scripts'
